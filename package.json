{
  "name": "ms-dag-ts",
  "version": "1.0.3",
  "description": "Directed Acyclic Graph",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "npm run build",
    "prepublish": "npm run build",
    "prebuild": "rimraf dist",
    "build": "tsc && npm run build:types",
    "build:types": "tsc -t esnext --moduleResolution node -d --emitDeclarationOnly --outFile dist/index.d.ts",
    "build:w": "tsc -w",
    "pretest": "npm run build",
    "test": "npm run tape",
    "tape": "tape dist/**/*.spec.js",
    "tape:w": "watch -p \"dist/**/*.js\" -c \"npm run -s tape\"",
    "pretdd": "npm run build",
    "tdd": "concurrently --raw \"npm run -s build:w\" \"npm run -s tape:w\""
  },
  "repository": "https://github.com/markschad/ms-dag-ts.git",
  "keywords": [],
  "author": "Mark Schad",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.19.10",
    "@types/tape": "^4.2.32",
    "concurrently": "^3.0.0",
    "rimraf": "^3.0.2",
    "tape": "^4.6.0",
    "typescript": "^4.2.4",
    "watch-cli": "^0.2.1"
  },
  "dependencies": {}
}
